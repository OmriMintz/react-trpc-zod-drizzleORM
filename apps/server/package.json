{
   "name": "@react-vite-trpc/server",
   "version": "1.0.0",
   "private": true,
   "scripts": {
      "start": "cross-env NODE_ENV=production node dist/src/server.js",
      "pm2:start": "pnpm pm2:delete && pnpm build && cross-env NODE_ENV=test pm2 start dist/src/server.js --name @react-vite-trpc/server",
      "pm2:delete": "pm2 delete all &",
      "pm2:logs": "pm2 logs",
      "dev": "nodemon src/server.ts",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "ts:check": "tsc -b",
      "check": "pnpm lint && pnpm ts:check",
      "build": "tsc -b"
   },
   "dependencies": {
      "@trpc/client": "^10.29.1",
      "@trpc/server": "^10.29.1",
      "@types/swagger-ui-express": "^4.1.6",
      "cookie-parser": "^1.4.6",
      "drizzle-orm": "^0.29.3",
      "drizzle-zod": "^0.5.1",
      "express": "^4.18.2",
      "express-serve-static-core": "^0.1.1",
      "mysql2": "^3.9.1",
      "openapi-types": "^12.1.3",
      "swagger-ui-express": "^5.0.0",
      "trpc-openapi": "^1.2.0",
      "uuid": "^9.0.1",
      "zod": "^3.21.4"
   },
   "devDependencies": {
      "@react-vite-trpc/config": "workspace:*",
      "@react-vite-trpc/tsconfig": "workspace:*",
      "@swc/core": "^1.3.62",
      "@trivago/prettier-plugin-sort-imports": "^4.2.0",
      "@types/cookie-parser": "^1.4.3",
      "@types/express": "^4.17.17",
      "@types/fs-extra": "^11.0.1",
      "@types/module-alias": "^2.0.1",
      "@types/node": "^18.16.17",
      "@vitest/coverage-istanbul": "^0.34.1",
      "cross-env": "^7.0.3",
      "dotenv": "^16.1.4",
      "drizzle-kit": "^0.20.14",
      "envalid": "^7.3.1",
      "fs-extra": "^11.1.1",
      "module-alias": "^2.2.3",
      "nodemon": "^2.0.22",
      "pm2": "^5.3.0",
      "ts-node": "^10.9.1",
      "typescript": "^4.9.5"
   }
